{
  "name": "cfdbadminton",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "proxy": "http://localhost:8080",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcingol1/cfdbadminton"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-latest": "^6.24.1",
    "cors": "^2.8.4",
    "graphql": "^14.0.2",
    "nodemon": "^1.18.5",
    "prisma-client-lib": "^1.19.3",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run server:watch\"",
    "build": "react-scripts build",
    "server": "node ./server/index.js",
    "server:watch": "nodemon --exec babel-node server/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "concurrently": "^4.0.1",
    "eslint-plugin-react-hooks": "0.0.0"
  }
}
